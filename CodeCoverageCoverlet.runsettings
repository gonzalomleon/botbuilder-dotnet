                <ModulePath>.*\Microsoft.Bot.Builder.Configuration.dll$</ModulePath>
                <ModulePath>.*\Microsoft.Bot.Builder.Connector.dll$</ModulePath>                
                <ModulePath>.*\Microsoft.Bot.Builder.Connector.Teams.dll$</ModulePath>                
