                <Source>.*\\DefaultAction.cs</Source>
                <Source>.*\\Element.cs</Source>
                <Source>.*\\FacebookAttachment.cs</Source>
                <Source>.*\\FacebookBotUser.cs</Source>
                <Source>.*\\FacebookEntry.cs</Source>
