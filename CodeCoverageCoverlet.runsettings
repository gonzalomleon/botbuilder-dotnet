                <ModulePath>.*\Microsoft.Bot.Builder.LanguageGeneration.dll$</ModulePath>                
                <!-- <ModulePath>Microsoft.Bot.Builder.Schema.dll$</ModulePath> -->
              </Include>
            </ModulePaths>
            <Sources>
              <Exclude>
                <Source>.*\\WebexMessageRequest.cs</Source>
